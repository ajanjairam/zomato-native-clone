{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "address": {
      "type": "text",
      "required": true
    },
    "rating": {
      "required": true,
      "max": 5,
      "min": 1,
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "dishes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dish.dish"
    }
  }
}
